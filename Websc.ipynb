        "#  ライブラリをインポート \n",
        "from bs4 import BeautifulSoup\n",
        "import requests\n",
        "url = \"https://dempagumi.tokyo/\"\n",
        "r = requests.get(url)\n",
        "soup = BeautifulSoup(r.text, \"lxml\")\n",
        "h2_list = soup.find_all('h2')\n",
        "for h2 in h2_list:\n",
        "  print(h2.text)"
      ],
